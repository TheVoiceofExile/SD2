<Window x:Class="SD2_APP.ServerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SD2_APP"
        mc:Ignorable="d"
        Title="ServerWindow" Height="400" Width="600">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Network Information" VerticalAlignment="Top"/>
        <TextBlock x:Name="NetworkIPDisplay" HorizontalAlignment="Left" Margin="10,31,0,0" TextWrapping="Wrap" Text="Network IP" VerticalAlignment="Top"/>
        <TextBlock x:Name="AppConfig" HorizontalAlignment="Left" Margin="10,52,0,0" TextWrapping="Wrap" Text="App Configuration" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="241,10,0,0" TextWrapping="Wrap" Text="Commands Sent" VerticalAlignment="Top"/>
        <ListBox x:Name="ConnectedClients" HorizontalAlignment="Left" Height="299" Margin="241,31,0,0" VerticalAlignment="Top" Width="159"/>

    </Grid>
</Window>
